{
  "name": "cogo-admin",
  "version": "0.0.1",
  "description": "Turbo repo for Cogo admin",
  "repository": "git@github.com:Cogoport/cogo-admin.git",
  "author": "Prashant Paddune <padadunep@gmail.com>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "*",
    "packages/*",
    "common/*"
  ],
  "scripts": {
    "deploy": "pnpm run prod:build && (cd cogo-control && pm2 start pm2.config.js)",
    "prod:build": "pnpm run pre-build && pnpm run build",
    "build": "NODE_ENV=production NODE_OPTIONS=\"--max-old-space-size=6144\" turbo run build",
    "pre-build": "NODE_ENV=production node scripts/build-pages && node scripts/pre-build",
    "dev": "node scripts/build-pages && node scripts/pre-build && turbo run dev",
    "start": "turbo run start",
    "eslint": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "eslint_file": "eslint",
    "stylelint": "stylelint \"**/*.css\"",
    "clean": "turbo run clean"
  },
  "dependencies": {
    "@cogoport/maps": "^1.0.2",
    "@sentry/nextjs": "^7.40.0",
    "axios-hooks": "^4.0.0",
    "next": "13.1.6",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@cogoport/eslint-config": "^1.1.0",
    "@cogoport/stylelint-config": "^1.0.3",
    "@svgr/webpack": "^6.5.1",
    "eslint": "^8.35.0",
    "fs-extra": "^11.1.0",
    "git-precommit-checks": "^3.1.0",
    "husky": "^8.0.3",
    "postcss": "^8.4.21",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-nested": "^6.0.1",
    "postcss-preset-env": "^8.0.1",
    "stylelint": "^14.16.1",
    "turbo": "^1.8.3"
  },
  "packageManager": "pnpm@7.15.0"
}
