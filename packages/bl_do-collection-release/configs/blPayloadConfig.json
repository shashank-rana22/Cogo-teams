{
    "knockoff_awaiting":{
        "common": {
          "filters": {
            "state": ["confirmed_by_importer_exporter", "in_progress", "shipment_received"],
            "is_knocked_off": false,
            "knockoff_awaiting": true
          },
          "additional_methods": ["pagination", "seller"]
        },
        "fcl_freight": {
          "export": {
            "filters": {
            "service_state": ["containers_gated_in", "vessel_departed", "vessel_arrived", "containers_gated_out"]
            }
          },
          "import": {
            "filters": {
              "service_state": ["vessel_departed", "vessel_arrived", "containers_gated_out"],
              "task_attributes": [{"task": "upload_igm_document", "status": "completed"},
               {"task": "knockoff_invoices", "status": "pending"}]
              }
          }
        },
        "lcl_freight": {
          "export": {
            "filters": {
            "service_state": ["containers_gated_in", "vessel_departed", "vessel_arrived", "containers_gated_out"]
            }
          },
          "import": {
            "filters": {
              "service_state": ["vessel_departed", "vessel_arrived", "containers_gated_out"],
              "task_attributes": [ {"task": "knockoff_invoices", "status": "pending"}]
              }
          }
        },
        "fcl_local": {
          "export": {
            "filters": {
            "service_state": ["containers_gated_in", "vessel_departed", "vessel_arrived", "containers_gated_out"]
            }
