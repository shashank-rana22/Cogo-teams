{
	"name": "@cogoport/service_discovery",
	"description": "Service Discovery",
	"version": "0.1.0",
	"main": "index.js",
	"sideEffects": false,
	"scripts": {
		"deploy": "pnpm run build && (cd cogo-control && pm2 start pm2.config.js)",
		"build": "pnpm run pre-build && NODE_ENV=production NODE_OPTIONS=\"--max-old-space-size=6144\" turbo run build",
		"pre-build": "NODE_ENV=production node scripts/build-pages && node scripts/pre-build",
		"dev": "npx husky install && node scripts/build-pages && node scripts/pre-build && turbo run dev",
		"start": "turbo run start",
		"eslint": "eslint \"**/*.{js,jsx,ts,tsx}\"",
		"eslint_file": "eslint",
		"stylelint": "stylelint \"**/*.css\"",
		"clean": "turbo run clean",
		"lint": "eslint",
		"precommit": "lint-staged"
	  },
	"dependencies": {
		"@cogoport/components": "^1.0.359",
		"@cogoport/forms": "workspace:*",
		"@cogoport/constants": "workspace:*",
		"@cogoport/globalization": "workspace:*",
		"@cogoport/core": "workspace:*",
		"@cogoport/icons-react": "^1.0.58",
		"@cogoport/next": "workspace:0.1.0",
		"@cogoport/request": "workspace:^0.1.0",
		"@cogoport/store": "workspace:^0.1.0",
		"@cogoport/utils": "^1.2.6",
		"axios": "^1.3.4",
		"date-fns": "^2.29.3",
		"axios-hooks": "^4.0.0",
		"react": "18.2.0"
	}
}
