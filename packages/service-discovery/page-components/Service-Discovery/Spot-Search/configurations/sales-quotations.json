{
	"heading": "Quotations",
	"api": "list_checkouts",
	"type": "quotations",
	"placement": "center",
	"href": "/quotation-approval",
	"as": "/quotation-approval",
	"extraParams": {
		"sales_dashboard_stats_required": true,
		"shipment_data_required": true
	},
	"fields": [
		{
			"key": "serial_id",
			"label": "ID",
			"func": "renderSerialId",
			"span": 1
		},
		{
			"label": "Service",
			"key": "primary_service",
			"func": "renderSearchType",
			"span": 1
		},
		{
			"label": "Shipper",
			"key": "importer_exporter.business_name",
			"topKey": {
				"key": "importer_exporter.business_name"
			},
			"lowerKey": {
				"key": "quotation_email_sent_by.name",
				"popoverKey": "quotation_email_sent_by",
				"withPopover": true
			},
			"func": "renderFieldPair",
			"span": 2
		},
		{
			"label": "Port pair",
			"key": "",
			"pair_type": "primary_service",
			"func": "renderPortPair",
			"span": 1.5
		},
		{
			"key": "shipment",
			"label": "Cargo/Load Details",
			"commodityKey": "quotation_cargo_details",
			"func": "renderShipment",
			"span": 2
		},
		{
			"key": "source",
			"label": "Source",
			"func": "renderSource",
			"span": 1
		},
		{
			"key": "created_at",
			"label": "Created at",
			"func": "renderCreated",
			"span": 1.5
		},
		{
			"key": "validity_end",
			"label": "Expiry",
			"func": "renderValidityEnd",
			"span": 1
		},
		{
			"key": "booking_status",
			"label": "Booking Status",
			"func": "renderBookingStatus",
			"span": 1
		}
	],
	"filterProps": {
		"searchKey": "serial_id",
		"searchPlaceholder": "Search by Serial Id",
		"controls": [
			{
				"name": "performed_by_type",
				"label": "Performed By Type",
				"type": "select",
				"multiple": false,
				"options": [
					{
						"label": "Organic",
						"value": "user"
					},
					{
						"label": "Inorganic",
						"value": "agent"
					}
				]
			},
			{
				"name": "selected_source",
				"label": "Select Source",
				"type": "select",
				"multiple": true,
				"options": [
					{
						"label": "Spot Search",
						"value": "spot_search"
					},
					{
						"label": "Contract",
						"value": "contract"
					},
					{
						"label": "Auto Quotation",
						"value": "auto_quotation"
					},
					{
						"label": "Cogo Assured Rate",
						"value": "cogo_assured_rate"
					},
					{
						"label": "Quick Checkout",
						"value": "quick_checkout"
					},
					{
						"label": "Sell Without Buy",
						"value": "direct"
					},
					{
						"label": "Pass Through",
						"value": "pass_through"
					},
					{
						"label": "Shipment Bulk Creation",
						"value": "shipment_bulk_operation"
					},
					{
						"label": "LTL Booking Request",
						"value": "ltl_booking_request"
					}
				]
			}
		]
	},
	"stats_key": "sales_dashboard_stats",
	"stats": [
		{
			"text": "All",
			"key": "total_count",
			"className": "green",
			"filter": {
				"is_email_quotation_sent": true,
				"action_name": "create",
				"source": ["spot_search", "contract", "auto_quotation", "cogo_assured_rate", "quick_checkout", "direct", "pass_through", "shipment_bulk_operation", "ltl_booking_request"]
			}
		},
		{
			"text": "Sent & not booked",
			"key": "total_sent",
			"filter": {
				"is_email_quotation_sent": true,
				"is_not_expired": true,
				"not_converted_to_booking": true,
				"action_name": "create",
				"source": ["spot_search", "contract", "auto_quotation", "cogo_assured_rate", "quick_checkout", "direct", "pass_through", "shipment_bulk_operation", "ltl_booking_request"]
			}
		},
		{
			"text": "BOOKED",
			"key": "booked",
			"className": "green",
			"filter": {
				"is_email_quotation_sent": true,
				"is_converted_to_booking": true,
				"action_name": "create",
				"source": ["spot_search", "contract", "auto_quotation", "cogo_assured_rate", "quick_checkout", "direct", "pass_through", "shipment_bulk_operation", "ltl_booking_request"]
			}
		},
		{
			"text": "EXPIRED",
			"key": "expired",
			"className": "red",
			"filter": {
				"is_email_quotation_sent": true,
				"not_converted_to_booking": true,
				"is_expired": true,
				"action_name": "create",
				"source": ["spot_search", "contract", "auto_quotation", "cogo_assured_rate", "quick_checkout", "direct", "pass_through", "shipment_bulk_operation", "ltl_booking_request"]
			}
		}
	]
}
