{
	"heading": "Shipments",
	"api": "list_shipments",
	"type": "sales_shipments",
	"href": "/shipment-management",
	"as": "/shipment-management",
	"extraParams": {
		"shipment_data_required": true,
		"stats_required": true,
		"last_completed_task_required": true
	},
	"fields": [
		{
			"key": "serial_id",
			"label": "ID",
			"func": "renderSerialId"
		},
		{
			"label": "Service",
			"key": "shipment_type",
			"func": "renderSearchType"
		},
		{
			"label": "Shipper",
			"key": "importer_exporter.business_name",
			"topKey": {
				"key": "importer_exporter.business_name"
			},
			"lowerKey": {
				"key": "quotation_email_sent_by.name",
				"popoverKey": "quotation_email_sent_by",
				"withPopover": true
			},
			"func": "renderFieldPair"
		},
		{
			"label": "Port pair",
			"key": "shipments_port_pair",
			"pair_type": "shipment_type",
			"func": "renderPortPair"
