		{
			"key": "shipment",
			"label": "Cargo/Load Details",
			"commodityKey": "shipment_cargo_details",
			"func": "renderShipment"
		},
		{
			"key": "source",
			"label": "Source",
			"func": "renderSource"
		},
		{
			"key": "created_at",
			"label": "Created at",
			"func": "renderCreated"
		},
		{
			"key": "last_mile_stone",
			"label": "Last MileStone",
			"func": "renderLastMileStone"
		},
		{
			"key": "next_mile_stone",
			"label": "UpComing Task",
			"func": "renderNextMileStone"
		},
		{
			"key": "shipment_status",
			"label": "Shipment Status",
			"func": "renderShipmentStatus"
		}
	],
	"defaultFilters": {
		"state": [
			"confirmed_by_importer_exporter",
			"in_progress",
			"shipment_received"
		]
	},
	"stats_key": "sales_dashboard_stats",
	"filterProps": {
		"searchKey": "serial_id",
		"searchPlaceholder": "Search by Serial Id",
		"controls": [
			{
				"name": "performed_by_type",
				"label": "Performed By Type",
				"type": "select",
				"multiple": false,
				"options": [
					{
						"label": "Organic",
						"value": "user"
					},
					{
						"label": "Inorganic",
						"value": "agent"
					}
				]
			},
			{
				"name": "source",
				"label": "Select Source",
				"type": "multi-select",
				"options": [
					{
						"label": "Spot Search",
						"value": "spot_search"
					},
					{
						"label": "Contract",
						"value": "contract"
					},
					{
						"label": "Auto Quotation",
						"value": "auto_quotation"
					},
					{
						"label": "Cogo Assured Rate",
						"value": "cogo_assured_rate"
					},
					{
						"label": "Quick Checkout",
						"value": "quick_checkout"
					},
					{
            			"label": "Sell Without Buy",
						"value": "direct"
					},
					{
						"label": "Pass Through",
						"value": "pass_through"
					},
					{
						"label": "Shipment Bulk Creation",
						"value": "shipment_bulk_operation"
					},
					{
						"label": "LTL Booking Request",
						"value": "ltl_booking_request"
					}
				]
			}
		]
	},
	"stats": [
		{
			"text": "Ongoing",
			"key": "on_going",
			"filter": {
				"state": [
					"shipment_received",
					"confirmed_by_importer_exporter",
					"in_progress"
				]
			}
		},
		{
			"text": "Completed",
			"key": "completed",
			"className": "green",
			"filter": {
				"state": "completed"
			}
		},
		{
			"text": "Cancelled",
			"key": "cancelled",
			"className": "red",
			"filter": {
				"state": "cancelled"
			}
		}
	]
}